@model IEnumerable<Barometer.Models.SubjectQuestions>

@{
    ViewBag.Title = "FillList";
}

<h2>FillList</h2>

<p>
    @ViewBag.ex
</p>

<div class="@ViewBag.Tag">
    <form name="@ViewBag.Tag" action="#" method=post enctype="multipart/form-data">
    <table>
        <tr>
            <td><label for="text">Vul de vragenlijst in:</label></td>
        </tr>
    </table>
    <table>
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Subject)
        </td>
    </tr>
    if(item.Questions != null){
        foreach (var item1 in item.Questions){
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item1.Name)
                </td>
            </tr>
        }
    }
}
</table>
</form>
</div>


